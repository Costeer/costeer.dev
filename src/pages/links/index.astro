---
import { getCollection } from "astro:content";
import Container from "@components/Container.astro";
import PageLayout from "@layouts/PageLayout.astro";
import Link from "@components/LinkN.astro";
import { dateRange } from "@lib/utils";
import { LINKS } from "@consts";
---

<PageLayout title={LINKS.TITLE} description={LINKS.DESCRIPTION}>
  <Container>
    <div class="space-y-10">

    <div class="flex flex-col center">
    <h1 class="animate font-ntype82 font-extrabold text-4xl text-center mb-6">Links</h1>


    <p class="animate font-Roboto text-fg text-center mt-4 mb-4 text-4xl" >
    <Link href="https://github.com/Costeer" aria-label="Astro Nano on Github" external>
    Github
    </Link>

    <p class="animate font-Roboto text-fg text-center mt-4 mb-4 text-4xl" >
    <Link href="https://steamcommunity.com/id/Costeeeeeeeeeer/" aria-label="Astro Nano on Github" external>
    Steam
    </Link>
    </p>

    <p class="animate font-Roboto text-fg text-center mt-4 mb-4 text-4xl" >
    <Link href="https://bsky.app/profile/costeer.dev" aria-label="Astro Nano on Github" external>
    BlueSky
    </Link>
    </p>

    <p class="animate font-Roboto text-fg text-center mt-4 mb-4 text-4xl" >
    <Link href="https://bsky.app/profile/costeer.dev" aria-label="Astro Nano on Github" external>
    Discord
    </Link>
    </p>

    </div>
  </Container>
</PageLayout>
